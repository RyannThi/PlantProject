3,{"$type":".General.CodeSegment, LuaSTGEditorSharp","Attributes":[{"attrCap":"Head","attrInput":"-- MenuInputChecker","EditWindow":"code"},{"attrCap":"Tail","attrInput":"","EditWindow":"code"}],"IsExpanded":false,"AttributeCount":2}
4,{"$type":".General.Code, LuaSTGEditorSharp","Attributes":[{"attrCap":"Code","attrInput":"function MenuInputChecker(name)\r\n    while(true) do\r\n        while(not KeyIsPressed(name))do\r\n            coroutine.yield(false) --return false until the key is pressed\r\n        end\r\n        coroutine.yield(true) --return true once\r\n        for i=0, 20 do\r\n            coroutine.yield(false) --return false for 30 frames\r\n            if (not KeyIsDown(name)) then\r\n                break --if the key is not being held down, break out of for (which will make you consequently restart\r\n            end\r\n        end\r\n        while (KeyIsDown(name)) do\r\n            coroutine.yield(true) -- return true once every 3 frames\r\n            for i=0, 2 do\r\n                coroutine.yield(false) --return false for 3 frames\r\n            end\r\n        end\r\n    end\r\nend","EditWindow":"code"}],"AttributeCount":1}
3,{"$type":".General.CodeSegment, LuaSTGEditorSharp","Attributes":[{"attrCap":"Head","attrInput":"-- SetWorldUEX","EditWindow":"code"},{"attrCap":"Tail","attrInput":"","EditWindow":"code"}],"IsExpanded":false,"AttributeCount":2}
4,{"$type":".General.Code, LuaSTGEditorSharp","Attributes":[{"attrCap":"Code","attrInput":"function SetWorldUEX(x, y, w, h, bound, m)\r\n\tbound = bound or 32\r\n\tm = m or 15\r\n\tOriginalSetWorld(\r\n\t--l,r,b,t,\r\n\t\t\t(-w / 2), (w / 2), (-h / 2), (h / 2),\r\n\t--bl,br,bb,bt\r\n\t\t\t(-w / 2) - bound, (w / 2) + bound, (-h / 2) - bound, (h / 2) + bound,\r\n\t--sl,sr,sb,st\r\n\t\t\t(x - w/2), (x + w/2), (y - h/2), (y + h/2),\r\n\t--pl,pr,pb,pt\r\n\t\t\t(-w / 2), (w / 2), (-h / 2), (h / 2),\r\n\t--world mask\r\n\t\t\tm\r\n\t)\r\n\tSetBound(lstg.world.boundl, lstg.world.boundr, lstg.world.boundb, lstg.world.boundt)\r\nend","EditWindow":"code"}],"AttributeCount":1}
3,{"$type":".General.CodeSegment, LuaSTGEditorSharp","Attributes":[{"attrCap":"Head","attrInput":"-- Wrap","EditWindow":"code"},{"attrCap":"Tail","attrInput":"","EditWindow":"code"}],"AttributeCount":2}
4,{"$type":".General.Code, LuaSTGEditorSharp","Attributes":[{"attrCap":"Code","attrInput":"function Wrap(x, x_min, x_max)\r\n\treturn (((x - x_min) % (x_max - x_min)) + (x_max - x_min)) % (x_max - x_min) + x_min;\r\nend","EditWindow":"code"}],"AttributeCount":1}
3,{"$type":".General.CodeSegment, LuaSTGEditorSharp","Attributes":[{"attrCap":"Head","attrInput":"-- Clamp","EditWindow":"code"},{"attrCap":"Tail","attrInput":"","EditWindow":"code"}],"AttributeCount":2}
4,{"$type":".General.Code, LuaSTGEditorSharp","Attributes":[{"attrCap":"Code","attrInput":"function Clamp(val, lower, upper)\r\n    assert(val and lower and upper, \"not very useful error message here\")\r\n    if lower > upper then lower, upper = upper, lower end -- swap if boundaries supplied the wrong way\r\n    return math.max(lower, math.min(upper, val))\r\nend","EditWindow":"code"}],"AttributeCount":1}
3,{"$type":".General.CodeSegment, LuaSTGEditorSharp","Attributes":[{"attrCap":"Head","attrInput":"-- Interpolation","EditWindow":"code"},{"attrCap":"Tail","attrInput":"","EditWindow":"code"}],"AttributeCount":2}
4,{"$type":".General.Code, LuaSTGEditorSharp","Attributes":[{"attrCap":"Code","attrInput":"function LerpDecel(a, b, x)\r\n    local y = 1 - x\r\n    return (a + (1 - y * y) * (b - a))\r\nend\r\n\r\nfunction Lerp(a, b, t)\r\n    return a + (b - a) * t\r\nend","EditWindow":"code"}],"AttributeCount":1}
